<?xml version="1.0"?>
<!DOCTYPE fontconfig SYSTEM "fonts.dtd">
<fontconfig>

<!--
 ! Sorry Symbola... but you're treadin' on my emoji!
 ! I've gotta keep you around for package dependency reasons, though.
 !-->

    <selectfont>
        <rejectfont>
            <pattern>
                <patelt name="family">
                    <string>Symbola</string>
                </patelt>
            </pattern>
        </rejectfont>
    </selectfont>

</fontconfig>
